{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "ui",
    "version": "0.1.0",
    "identifier": "com.ohmsl.clipui",
    "build": {
        "beforeDevCommand": "pnpm web",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "ui",
                "width": 1000,
                "height": 975
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "nsis",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
